<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

<listeners>
<listener class-name = "priyaseleniumlearning.testcomponents.Listeners"/>
</listeners>

  <test thread-count="5" name="Base Test">
    <classes>
      <class name="priyaseleniumlearning.testcomponents.BaseTest"/>
    </classes>
  </test> <!-- Test -->

<test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="priyaseleniumlearning.tests.ErrorValidationTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="priyaseleniumlearning.tests.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->  
</suite> <!-- Suite -->